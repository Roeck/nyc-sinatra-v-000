<h2>Edit <%= @landmark.name %></h2>
<form action="/landmarks/<%= @landmark.id %>/edit" method="post">
  <input type="hidden" name="_method" value="PATCH">
  <label for="name">Landmark Name</label>
  <input id="name" type="text" name="landmark[name]" value="<%= @landmark.name %>">
  <br><br>
  <label for="year_completed">Year of completion</label>
  <input id="year_completed" type="text" name="landmark[year_completed]" value="<%= @landmark.year_completed %>">
  <br><br>
  <input type="submit" value="Edit Landmark">
</form>
